<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/search"
    app:startDestination="@id/searchFragment">

    <fragment android:id="@+id/searchFragment"
        android:name="io.dublink.search.SearchFragment"
        android:label="SearchFragment">
        <action
            android:id="@id/searchFragment_to_livedata"
            app:destination="@id/livedata" />
        <action
            android:id="@id/searchFragment_to_settings"
            app:destination="@id/settings" />
    </fragment>

    <include app:graph="@navigation/livedata" />

    <include app:graph="@navigation/settings" />

</navigation>
