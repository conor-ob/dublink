<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/settings"
    app:startDestination="@id/settingsFragment">

    <fragment android:id="@+id/settingsFragment"
        android:name="io.dublink.settings.SettingsFragment"
        android:label="SettingsFragment">
        <action
            android:id="@+id/settingsFragment_to_dubLinkProFragment"
            app:destination="@id/dubLinkProFragment" />
        <action
            android:id="@+id/settingsFragment_to_webViewFragment"
            app:destination="@id/webViewFragment" />
    </fragment>

    <fragment android:id="@+id/dubLinkProFragment"
        android:name="io.dublink.iap.dublinkpro.DubLinkProFragment"
        android:label="DubLinkProFragment" />

    <fragment android:id="@+id/webViewFragment"
        android:name="io.dublink.web.WebViewFragment"
        android:label="WebViewFragment" />

</navigation>
