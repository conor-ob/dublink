<resources>
    <style name="Widget.DublinMapper.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="titleTextAppearance">?attr/textAppearanceHeadline6</item>
        <item name="subtitleTextAppearance">?attr/textAppearanceSubtitle2</item>
    </style>

    <style name="Widget.DublinMapper.RecyclerView.FastScroll" parent="android:Widget">
        <item name="fastScrollEnabled">true</item>
        <item name="fastScrollHorizontalThumbDrawable">@drawable/recyclerview_scroll_thumb</item>
        <item name="fastScrollHorizontalTrackDrawable">@drawable/recyclerview_scroll_track</item>
        <item name="fastScrollVerticalThumbDrawable">@drawable/recyclerview_scroll_thumb</item>
        <item name="fastScrollVerticalTrackDrawable">@drawable/recyclerview_scroll_track</item>
    </style>

    <style name="Widget.DublinMapper.HeaderHolderFrameLayout" parent="">
<!--        <item name="dividerDrawable">?android:attr/listDivider</item>-->
<!--        <item name="materialBackgroundColor">?attr/colorSurface</item>-->
    </style>
    
    <style name="TextAppearance.DublinMapper.Headline1" parent="TextAppearance.MaterialComponents.Headline1">
        <item name="fontFamily">@font/barlow_regular</item>
        <item name="android:fontFamily">@font/barlow_regular</item>
        <item name="android:textSize">101.03sp</item>
        <item name="android:letterSpacing">-0.15</item>
    </style>

    <style name="TextAppearance.DublinMapper.Headline2" parent="TextAppearance.MaterialComponents.Headline2">
        <item name="fontFamily">@font/barlow_regular</item>
        <item name="android:fontFamily">@font/barlow_regular</item>
        <item name="android:textSize">63.15sp</item>
        <item name="android:letterSpacing">-0.005</item>
    </style>

    <style name="TextAppearance.DublinMapper.Headline3" parent="TextAppearance.MaterialComponents.Headline3">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">50.12sp</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="TextAppearance.DublinMapper.Headline4" parent="TextAppearance.MaterialComponents.Headline4">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">35.5sp</item>
        <item name="android:letterSpacing">0.0025</item>
    </style>

    <style name="TextAppearance.DublinMapper.Headline5" parent="TextAppearance.MaterialComponents.Headline5">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">25.06sp</item>
        <item name="android:letterSpacing">0</item>
    </style>

    <style name="TextAppearance.DublinMapper.Headline6" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">20.76sp</item>
        <item name="android:letterSpacing">0.0025</item>
    </style>

    <style name="TextAppearance.DublinMapper.Body1" parent="TextAppearance.MaterialComponents.Body1">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">16.71sp</item>
        <item name="android:letterSpacing">0.005</item>
    </style>

    <style name="TextAppearance.DublinMapper.Body2" parent="TextAppearance.MaterialComponents.Body2">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">14.62sp</item>
        <item name="android:letterSpacing">0.0025</item>
    </style>

    <style name="TextAppearance.DublinMapper.Subtitle1" parent="TextAppearance.MaterialComponents.Subtitle1">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">16.71sp</item>
        <item name="android:letterSpacing">0.0015</item>
    </style>

    <style name="TextAppearance.DublinMapper.Subtitle2" parent="TextAppearance.MaterialComponents.Subtitle2">
        <item name="fontFamily">@font/barlow_regular</item>
        <item name="android:fontFamily">@font/barlow_regular</item>
        <item name="android:textSize">14.62sp</item>
        <item name="android:letterSpacing">0.001</item>
    </style>

    <style name="TextAppearance.DublinMapper.Button" parent="TextAppearance.MaterialComponents.Button">
        <item name="fontFamily">@font/barlow_bold</item>
        <item name="android:fontFamily">@font/barlow_bold</item>
        <item name="android:textSize">14.53sp</item>
        <item name="android:letterSpacing">0.0125</item>
    </style>

    <style name="TextAppearance.DublinMapper.Caption" parent="TextAppearance.MaterialComponents.Caption">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">12.53sp</item>
        <item name="android:letterSpacing">0.004</item>
    </style>

    <style name="TextAppearance.DublinMapper.Overline" parent="TextAppearance.MaterialComponents.Overline">
        <item name="fontFamily">@font/barlow_medium</item>
        <item name="android:fontFamily">@font/barlow_medium</item>
        <item name="android:textSize">12.53sp</item>
        <item name="android:letterSpacing">0.2</item>
    </style>

    <style name="TextAppearance.DublinMapper.PopupMenu.Header" parent="TextAppearance.DublinMapper.Button">
        <item name="android:textColor">?android:attr/textColorSecondary</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style parent="Widget.MaterialComponents.Chip.Action" name="ThinnerChip">
        <item name="chipCornerRadius">5dp</item>
        <item name="chipMinHeight">0dp</item>
        <item name="chipStartPadding">5dp</item>
        <item name="iconStartPadding">0dp</item>
        <item name="iconEndPadding">0dp</item>
        <item name="textStartPadding">0dp</item>
        <item name="textEndPadding">0dp</item>
        <item name="closeIconStartPadding">0dp</item>
        <item name="closeIconEndPadding">0dp</item>
        <item name="chipEndPadding">5dp</item>
        <item name="chipMinTouchTargetSize">0dp</item>
        <item name="android:textAppearance">@style/SmallerText</item>
    </style>

    <style parent="TextAppearance.AppCompat.Small" name="SmallerText">
        <item name="fontFamily">@font/barlow_bold</item>
        <item name="android:fontFamily">@font/barlow_bold</item>
        <item name="android:textSize">15sp</item>
    </style>
</resources>

