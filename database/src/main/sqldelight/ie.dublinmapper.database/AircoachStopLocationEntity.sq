CREATE TABLE aircoachStopLocationEntity (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL
);

selectAll:
SELECT * FROM aircoachStopLocationEntity;

insertOrReplace:
INSERT OR REPLACE INTO aircoachStopLocationEntity(
  id,
  name,
  latitude,
  longitude
)
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM aircoachStopLocationEntity;
