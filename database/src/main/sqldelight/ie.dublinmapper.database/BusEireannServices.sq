import io.rtpi.api.Operator;

CREATE TABLE busEireannServices (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    locationId TEXT NOT NULL,
    route TEXT NOT NULL,
    operator TEXT AS Operator NOT NULL,
    FOREIGN KEY (locationId) REFERENCES busEireannLocations(id)
);

selectAll:
SELECT * FROM busEireannServices;

insertOrReplace:
INSERT OR REPLACE INTO busEireannServices(
  locationId,
  route,
  operator
)
VALUES (?, ?, ?);

deleteAll:
DELETE FROM busEireannServices;
