CREATE TABLE busEireannStopLocationEntity (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL
);

selectAll:
SELECT * FROM busEireannStopLocationEntity;

insertOrReplace:
INSERT OR REPLACE INTO busEireannStopLocationEntity(
  id,
  name,
  latitude,
  longitude
)
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM busEireannStopLocationEntity;
