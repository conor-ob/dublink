CREATE TABLE dublinBikesLocations (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL
);

selectAll:
SELECT * FROM dublinBikesLocations;

insertOrReplace:
INSERT OR REPLACE INTO dublinBikesLocations(
  id,
  name,
  latitude,
  longitude
)
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM dublinBikesLocations;
