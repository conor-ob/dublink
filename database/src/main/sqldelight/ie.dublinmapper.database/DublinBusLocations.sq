CREATE TABLE dublinBusLocations (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL
);

selectAll:
SELECT * FROM dublinBusLocations;

insertOrReplace:
INSERT OR REPLACE INTO dublinBusLocations(
  id,
  name,
  latitude,
  longitude
)
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM dublinBusLocations;
