CREATE TABLE irishRailLocations (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    latitude REAL NOT NULL,
    longitude REAL NOT NULL
);

selectAll:
SELECT * FROM irishRailLocations;

insertOrReplace:
INSERT OR REPLACE INTO irishRailLocations(
  id,
  name,
  latitude,
  longitude
)
VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM irishRailLocations;
