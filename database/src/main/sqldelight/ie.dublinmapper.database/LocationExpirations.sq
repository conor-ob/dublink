import io.rtpi.api.Service;
import java.time.Instant;

CREATE TABLE locationExpirations (
    service TEXT AS Service NOT NULL PRIMARY KEY,
    lastUpdated TEXT AS Instant NOT NULL
);

selectById:
SELECT * FROM locationExpirations WHERE service = ?;

insertOrReplace:
INSERT OR REPLACE INTO locationExpirations(
  service,
  lastUpdated
)
VALUES (?, ?);
