import io.rtpi.api.Operator;

CREATE TABLE luasStopServiceEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    operator TEXT AS Operator NOT NULL,
    route TEXT NOT NULL,
    locationId TEXT NOT NULL,
    FOREIGN KEY (locationId) REFERENCES luasStopLocationEntity(id)
);

selectAll:
SELECT * FROM luasStopServiceEntity;

insertOrReplace:
INSERT OR REPLACE INTO luasStopServiceEntity(
  operator,
  route,
  locationId
)
VALUES (?, ?, ?);

deleteAll:
DELETE FROM luasStopServiceEntity;
